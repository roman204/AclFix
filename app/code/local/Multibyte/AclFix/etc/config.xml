<?xml version="1.0"?>
<config>
    <modules>
        <Multibyte_AclFix>
            <version>1.0.0</version>
        </Multibyte_AclFix>
    </modules>
    <global>
        <helpers>
            <multibyte_aclfix>
                <class>Multibyte_AclFix_Helper</class>
            </multibyte_aclfix>
        </helpers>
        <models>
            <multibyte_aclfix>
                <class>Multibyte_AclFix_Model</class>
            </multibyte_aclfix>
        </models>
        <events>
            <adminhtml_controller_action_predispatch_start>
                <observers>
                    <multibyte_aclfix>
                        <class>Multibyte_AclFix_Model_Observer</class>
                        <method>adminhtmlControllerActionPredispatchStart</method>
                    </multibyte_aclfix>
                </observers>
            </adminhtml_controller_action_predispatch_start>
        </events>
    </global>
</config>